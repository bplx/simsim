// includes
#include <stdio.h>
#include <stdbool.h>
#include <string.h>
#include <stdlib.h>

// includes for system-specific functions
#ifdef _WIN32
#include <Windows.h>
#else
#include <unistd.h>
#endif

char getprompt() {
	char buf[101];
	scanf("%s", buf);
	char* ans = (char *) malloc(strlen(ans) + 1);
	strcpy(ans, buf);
	return ans;
	free(ans);
}

int main() {
	printf("simsim v1");
	bool gamerunning;
	gamerunning = true;
	while (gamerunning) {
		char ans[3];
		strcpy(ans, getprompt());
	}
}
